<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	
    <services>
    	
    	<!-- Builder Service -->
        <service id="quick_coding.builder" class="Fire01\QuickCodingBundle\Builder" autowire="true" autoconfigure="true" public="false">
        </service>

        <service id="Fire01\QuickCodingBundle\Builder" alias="quick_coding.builder" />
        <!-- End Builder Service -->
    	
    	
    	<!-- Maker Service -->
		<service id="Fire01\QuickCodingBundle\Command\MakeController" autoconfigure="true">
		    <argument type="service" id="maker.doctrine_helper" />
		</service>
		<!-- End Maker Service -->
    </services>
    
</container>